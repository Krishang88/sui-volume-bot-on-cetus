# =============================================================================
# SUI Volume Booster Bot Configuration
# =============================================================================
# Copy this file to .env and configure your settings
# cp .env.example .env

# =============================================================================
# NETWORK CONFIGURATION
# =============================================================================

# Target network (mainnet or testnet)
NETWORK=mainnet

# SUI RPC endpoint for blockchain interactions
RPC_ENDPOINT=https://fullnode.mainnet.sui.io

# WebSocket endpoint for real-time updates (optional)
RPC_WEBSOCKET_ENDPOINT=wss://fullnode.mainnet.sui.io

# =============================================================================
# WALLET CONFIGURATION
# =============================================================================

# Your main wallet private key (keep this secure!)
PRIVATE_KEY=your_wallet_private_key_here

# =============================================================================
# TRADING PARAMETERS
# =============================================================================

# Cetus pool ID for trading
POOL_ID=your_cetus_pool_id_here

# Minimum SUI amount for each swap (in SUI)
SWAP_AMOUNT_MIN=1

# Maximum SUI amount for each swap (in SUI)
SWAP_AMOUNT_MAX=10

# Minimum time interval between buy operations (in seconds)
BUY_INTERVAL_MIN=30

# Maximum time interval between buy operations (in seconds)
BUY_INTERVAL_MAX=120

# Minimum time interval between wallet distributions (in seconds)
DISTRIBUTE_INTERVAL_MIN=60

# Maximum time interval between wallet distributions (in seconds)
DISTRIBUTE_INTERVAL_MAX=300

# =============================================================================
# ADVANCED SETTINGS
# =============================================================================

# Enable debug mode for detailed logging
DEBUG=false

# Gas budget for transactions (in MIST)
GAS_BUDGET=10000000

# Priority fee for transactions (in MIST)
PRIORITY_FEE=1000

# Maximum number of concurrent wallets
MAX_WALLETS=10

# =============================================================================
# SECURITY SETTINGS
# =============================================================================

# Enable transaction confirmation prompts
CONFIRM_TRANSACTIONS=false

# Maximum total volume per hour (in SUI)
MAX_HOURLY_VOLUME=1000

# Emergency stop flag
EMERGENCY_STOP=false
